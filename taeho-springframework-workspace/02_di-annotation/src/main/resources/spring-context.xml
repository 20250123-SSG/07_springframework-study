<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">

  <bean class="com.johnth.di_annotation.dto.Phone" id="phone1">
    <property name="name" value="아이폰 16 pro"/>
    <property name="price" value="1000000" />
    <property name="brand" value="Apple"/>
    <property name="releaseDate" value="2025-05-01"/>
  </bean>

  <bean class="com.johnth.di_annotation.dto.Phone" id="phone2">
    <property name="name" value="갤럭시 프로"/>
    <property name="price" value="1050000" />
    <property name="brand" value="Banana"/>
    <property name="releaseDate" value="2025-04-01"/>
  </bean>


  <context:component-scan base-package="com.johnth.di_annotation"/>
</beans>